<h1>Sign in</h1>

<%= form_tag user_session_path, data: { turbo: false } do %>
  <div class="field">
    <%= label_tag "user_session[email]", "Email" %>

    <%= email_field_tag "user_session[email]" %>
  </div>

  <div class="field">
    <%= label_tag "user_session[password]", "Password" %>

    <%= password_field_tag "user_session[password]" %>
  </div>

  <%= submit_tag "Sign in", class: "button" %>
<% end %>